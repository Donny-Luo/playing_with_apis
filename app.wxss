.container {
  min-height: 100vh;
  background: #F8FEFF;
  display: flex;
  flex-direction: column;
  align-items: center;
  padding: 120rpx 40rpx;
  box-sizing: border-box;
  font-size: 40rpx;
  font-weight: lighter;
}

.btn {
  width: 80vw;
  height: 100rpx;
  display: flex;
  align-items:center;
  justify-content: center;
  color: white;
  border-radius: 4px;
  position: absolute;
  bottom: 120rpx;
  font-size: 40rpx;
  font-weight: normal;
}
